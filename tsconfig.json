{
    "compilerOptions": {
        "alwaysStrict": true,
        "module": "ESNext",
        "strict": true,
        "target": "es5",
        "lib": ["es6", "dom"],
        "declaration": true,
        "importHelpers": true,
        "moduleResolution": "node",
        "noImplicitAny": false,
        "downlevelIteration": true,
        "esModuleInterop": true
    }
}
